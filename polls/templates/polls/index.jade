- load staticfiles
- load compress

| {% compress css %}
link(rel="stylesheet", type="text/scss", href="{% static 'polls/style.scss' %}")
| {% endcompress %}

if latest_question_list
  ul
    - for question in latest_question_list
      li
        b
          a(href="{% url 'polls:detail' question.id %}")= question.question_text
else
  p No polls are available.
